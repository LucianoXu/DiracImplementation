(VAR a b c x S0 S1 S2 S3 s t B0 K0)
(SIG (0 0) (1 0) (+ 2) (@ 2) (conj 1) (C 1) (delta 2) (ADDS 2) (MLTS 2) (CONJS 1) (DOT 2) (0K 0) (ket 1) (CONJK 1) (TRANK 1) (SCRK 2) (ADDK 2) (MLTK 2) (TSRK 2) (0B 0) (bra 1) (CONJB 1) (TRANB 1) (SCRB 2) (ADDB 2) (MLTB 2) (TSRB 2) (0O 0) (OUTER 2) (CONJO 1) (TRANO 1) (SCRO 2) (ADDO 2) (MLTO 2) (TSRO 2))
(RULES
   +(0,a) -> a
   @(0,a) -> 0
   @(1,a) -> a
   @(a,+(b,c)) -> +(@(a,b),@(a,c))
   conj(0) -> 0
   conj(1) -> 1
   conj(+(a,b)) -> +(conj(a),conj(b))
   conj(@(a,b)) -> @(conj(a),conj(b))
   conj(conj(a)) -> a
   +(+(0,a),x) -> +(a,x)
   @(@(0,a),x) -> @(0,x)
   @(@(1,a),x) -> @(a,x)
   @(@(a,+(b,c)),x) -> @(+(@(a,b),@(a,c)),x)
   ADDS(C(a),C(b)) -> C(+(a,b))
   ADDS(S0,S0) -> MLTS(C(+(1,1)),S0)
   MLTS(S1,ADDS(S2,S3)) -> ADDS(MLTS(S1,S2),MLTS(S1,S3))
   CONJS(delta(s,t)) -> delta(s,t)
   CONJS(ADDS(S1,S2)) -> ADDS(CONJS(S1),CONJS(S2))
   CONJS(MLTS(S1,S2)) -> MLTS(CONJS(S1),CONJS(S2))
   CONJS(CONJS(S0)) -> S0
   CONJS(DOT(B0,K0)) -> DOT(CONJB(B0),CONJK(K0))
)
