operators
  0 : constant
  s : unary
  minus : binary
  quot : binary
  x,y,z : variable

 
axioms

  minus(x,0) = x ;
  minus(s(x),s(y)) = minus(x,y) ;
  quot(0,s(y)) = 0 ;
  quot(s(x),s(y)) = s(quot(minus(x,y),s(y))) ;

order

  lexico (
    rps( 
      rpo(quot > s ; quot lrlex );
      minus(x,y) -> x 
      );
    rpo())

end

