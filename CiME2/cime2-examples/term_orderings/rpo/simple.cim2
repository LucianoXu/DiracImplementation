let F = signature "
0 : constant;
s : unary;
+,* : infix binary;
";

let X = vars "x y z";

let p = precedence F " * > + > s > 0";

let o = MPO p;

let fun test t1 t2 = 
  term_compare o (term F X t1) (term F X t2);

test "x + 0" "x";
test "x + s(y)" "x + y";
test "x + s(y)" "s(x + y)";
test "x * 0" "0";
test "x * s(y)" "(x*y) + x";
