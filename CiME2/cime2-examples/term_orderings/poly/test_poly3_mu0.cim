operators
  +,* : AC
  0 : constant
  s : unary
  x,y,z : variable

axioms
  
  x+0 = x ;
  s(x)+y = s(x+y) ;
  0*x = 0 ;
  s(x)*y = (x*y)+y ;
  x*(y+z) = (x*y)+(x*z) ;

order
  lexico(    
    poly( [0]=0; [s](x)=x+2; 
          [+](x,y)=x+y+3; [*](x,y)=x.y+2.(x+y)+2 ) ;
    poly( [0]=0; [s](x)=x+1; 
          [+](x,y)=x.y+2.(x+y)+2;   [*](x,y)=x.y+2.(x+y)+2) )


end


