operators
  +,* : AC
  0,1 : constant
  - : unary
  x,y,z : variable

axioms
  
  x+0 = x ;
  x+(-(x)) = 0 ;
  (-(0)) = 0 ;
  -(-(x)) = x ;
  -(x+y) = (-(x))+(-(y)) ;
  x*1 = x ;
  x*(y+z) = (x*y)+(x*z) ;
  x*0 = 0 ;
  x*(-(y)) = -(x*y) ;

order
  
  poly (2; 
    [0]=2; [1]=2 ; 
    [+](x,y)=x+y+4 ; [*](x,y)=x.y+2.(x+y)+2 ; [-](x)=2.x+3 ) 
   
end


