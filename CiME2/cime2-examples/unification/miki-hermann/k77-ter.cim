
%
% Perfect matching K6,6 - (6 * K1,1) graph
%

operators
  + : AC
  0,a,b,c,d,e,f,g : constant
  - : unary
  h : unary
  x1,x2,x3,x4,x5,x6,x7 : variable

%unification theory  AG(+,0,-)

unification type plain

problems

  unify h(x1) + h(x2) + h(x3) + h(x4) + h(x5) + h(x6) + h(x7) =
        h(a) + h(b) + h(c) + h(d) + h(e) + h(f) + h(g);

end
