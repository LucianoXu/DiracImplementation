operators
  xor, and : AC
  false, true : constant
  x,y,z : variable
  not : unary

axioms

  x and true = x ;
  x and false = false ;

  x xor x = false ;
  x xor false = x ;
  x and (y xor z) = (x and y) xor (x and z);
  
  not(x) xor x = true;

order  lexico ( mapo ( and  > not > xor > true = false );
                poly([and](x,y)=x.y ; [xor](x,y)=x+y+1))

end





