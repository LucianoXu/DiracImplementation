%***************************************************************************
%
%
% Comple'tion du groupe $S_3$
%
%
%***************************************************************************

sort
  s

operators
  . : infix binary as s -> s -> s
  e : constant as s
  a,b : constant as s

axioms
   b.b = e ;
   a.a = e ;
   a.(b.(a.(b.(a.b)))) = b.(a.(b.(a.(b.a)))) ;

inductive
     a = b ;

end
