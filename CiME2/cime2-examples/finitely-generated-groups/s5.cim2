(* Completion of S5 *)

(* The transpositions *)

let F = word_signature "c t";

(* An order *)
let o = multi_lex (word_precedence F "c>t");

(* The string rewriting system *)

let R = SRS F "
  c ^ 5 ->;
  t ^2 ->;
  c (t c)^3 t -> ;

";

(* The completion *)

let S5 = word_completion o R ;

word_counter S5;

