(* Example borrowed from G. Karpilosky, Schur multipliers *)
(* G1 is isomorphic to S3 *)


let F = word_signature "a b a' b'";

let R = SRS F "
a^3 -> ;
b^2 -> ;
b a b' -> a^2;
a a' -> ;
a' a -> ;
b b' -> ;
b' b -> ;
";

let o = length_lex (word_precedence F "b' > a' > b > a");

#time;

let G1 = word_completion o R ;

#mem;

"the following line should display 6";
word_counter G1; 
