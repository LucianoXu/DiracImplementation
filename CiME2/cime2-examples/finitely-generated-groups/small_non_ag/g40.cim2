(* Example borrowed from G. Karpilosky, Schur multipliers *)
(* G40 *)

let F = word_signature "a b a' b'" ;

let R = SRS F "
a^3 -> ;
a' b a -> b'^2 ;
a a' -> ;
a' a -> ;
b b' -> ;
b' b -> ;
";

let o = length_lex (word_precedence F "b' > a' > b > a");

#time;

let G40 = word_completion o R ;

#mem;

word_counter G40; (* should be 27 *)
