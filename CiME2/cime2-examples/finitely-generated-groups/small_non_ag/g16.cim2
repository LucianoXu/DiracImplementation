(* Example borrowed from G. Karpilosky, Schur multipliers *)
(* G16 *)

let F = word_signature "a b a' b' c c' ";

let R = SRS F "
a^2 -> ;
b^2 -> ;
c^2 -> ;
a b c -> b c a ;
a b c -> c a b ;
b c a -> c a b ;
a a' -> ;
a' a -> ;
b b' -> ;
b' b -> ;
";

let o = length_lex (word_precedence F "c' > b' > a' > c > b > a");

#time;

let G16 = word_completion o R ;

#mem;

word_counter G16; (* should be 16 *)
