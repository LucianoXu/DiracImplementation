(* Example borrowed from G. Karpilosky, Schur multipliers *)
(* G7 is isomorphic to A4 *)

let F = word_signature "a b a' b'";

let R = SRS F "
a^6 -> ;
b^2 -> a^3;
b a' b' -> a ^ 5 ;
a a' -> ;
a' a -> ;
b b' -> ;
b' b -> ;
";

let o = length_lex (word_precedence F "b' > a' > b > a");

#time;

let G7 = word_completion o R ;

#mem;

word_counter G7; (* should be 12 *)
