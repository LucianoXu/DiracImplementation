(* Example borrowed from G. Karpilosky, Schur multipliers *)
(* G5 is isomorphic to D6 and $\mu_2 \times \sigma_3$ *)

let F = word_signature "a b a' b'";

let R = SRS F "
a^6 -> ;
b^2 -> ;
b a b' -> a^5;
a a' -> ;
a' a -> ;
b b' -> ;
b' b -> ;
";

let o = length_lex (word_precedence F "b' > a' > b > a");

#time;

let G5 = word_completion o R ;

#mem;

word_counter G5; (* should be 12 *)
