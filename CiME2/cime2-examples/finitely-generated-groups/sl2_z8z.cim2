(* $SL_2(Z/8Z)$ *)

let F = word_signature "s t" ;

let R = SRS F "
s^4 -> ;
s^2 t -> t s^2 ;
(s t)^3 -> s^2 ;

t^8 -> ;

s t^3 s -> t^2 s t^5 s t^2 ;

";

let o = multi_lex (word_precedence F "s > t");

#time;
#verbose;

let SL = word_completion o R ;

#mem;

word_counter SL; (* should be 192 *)


