%***************************************************************************
%
%
% Comple'tion du groupe $S_3$
%
%
%***************************************************************************

sort
  s

operators
  . : infix binary as s -> s -> s
  e : constant as s
  a,b : constant as s

axioms


   a.a = e ;
   b.(b.b) = e ;
   b.(a.b) = a ;
   b.(b.a) = a.b ;
   a.(b.b) = b.a ;
   a.(b.a) = b.b ;

inductive
     a = b ;

end
