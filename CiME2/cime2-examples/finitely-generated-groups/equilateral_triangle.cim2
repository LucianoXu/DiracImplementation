(* Completion of th group mentionned in LE CHennadec "Canonical forms in Finitely Presented Algebras" *)

let F = word_signature "S T";

(* Orders *)
let o = multi_lex (word_precedence F "S < T ");


(* The string rewriting system *)

let R = SRS F "
S^2 -> ;
T^2 -> ;
(S T)^3 -> ; 
";

(* The completion *)
 
let C = word_completion o R ;


let number_of_elements = word_counter C;


