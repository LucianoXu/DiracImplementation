sort
  int,bool

operators
   0 : 0 as int 
   s : 1 as int->int 
   p : 1 as int -> int;
   i : 2 as int -> int -> bool;
   false : 0 as bool;
   true : 0 as bool;
   x,z  : variable as int;

axioms

  s(p(x)) = x ;
  p(s(x)) = x ;

  i(0,0) = false ;
  i(s(x),z) = i(x,p(z)) ;
  i(p(x),z) = i(x,s(z)) ;

inductive
  i(s(0),x) = true ;
end
