(*

 Rubio example 1 (initially for MSPO)

*)


let F = signature "
a,b : constant;
f,g,h : unary;
";

let X = vars "";

let R = TRS F X "
a -> b;
f(b) -> g(a);
h(a) -> h(f(b));
";

#time on;
termination R;
termcrit "dp";
termination R;
termcrit "nograph";
termination R;
termcrit "nomarks";
termination R;
termcrit "graph";
termination R;
#time off;

