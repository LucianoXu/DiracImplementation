(*

 From [Ferreira & Zantema, dummy elimination]

*)


let F = signature "
f,g,h : unary;
";

let X = vars "x";

let R = TRS F X "
f(g(x)) -> g(f(f(x)));
f(h(x)) -> h(g(x));
";

#time on;
termination R;
termcrit "dp";
termination R;
termcrit "nograph";
termination R;
termcrit "nomarks";
termination R;
termcrit "graph";
termination R;
#time off;

