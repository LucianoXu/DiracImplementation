let Var = vars "X Y";

let F_4 = signature "
  a, b, c : 0;
  s : 1;
  f : 2;
";

let test_4 = HTRS {} F_4 Var "
  f(a,a) -> f(a,b);
  f(a,b) -> f(s(a),c);
  f(s(X),c) -> f(X,c);
  f(c,c) -> f(a,a);
";


termcrit "dp" ; 
termcrit "minimal" ; 
polyinterpkind {("linear",1)} ;
#time on;
h_termination test_4;



let F_829 = signature "
  f, g, s : 1;
  c : 2;
";

let test_829 = HTRS {} F_829 Var "
f(c(X,s(Y))) -> f(c(s(X),Y));
g(c(s(X),Y)) -> f(c(X,s(Y)));
";

termcrit "dp" ; 
termcrit "minimal" ; 
polyinterpkind {("linear",1)} ;
#time on;
h_termination test_829;



let F_830d = signature "
  0 : 0;
  f, g, h, s : 1;
  cons : 2;
";

let test_830d = HTRS {} F_830d Var "
  f(s(X))  -> f(X);
  g(cons(0,Y))  -> g(Y);
  g(cons(s(X),Y))  -> s(X);
  h(cons(X,Y))  -> h(g(cons(X,Y)));
";

termcrit "dp" ; 
termcrit "minimal" ; 
polyinterpkind {("linear",1)} ;
#time on;
h_termination  test_830d;

#quit;



