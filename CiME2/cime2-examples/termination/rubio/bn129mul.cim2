let Var  = vars "X X1 X2 X3 X4 Y Z";

let F = signature "
s : 1;
plus : 2;
";

let R = HTRS {} F Var "
plus(s(X),plus(Y,Z)) -> plus(X,plus(s(s(Y)),Z));
plus(s(X1),plus(X2,plus(X3,X4))) -> plus(X1,plus(X3,plus(X2,X4)));
";


termcrit "minimal";
polyinterpkind {("linear",1)};
#time on;
h_termination R;

#quit;
