
let N = parameters "N";

let S = pword_signature N "S | {N>=2}; T | {N>=2}" ;

let R = psrs S "S S -> | {N>=2};
 		(S T)^{N-2} -> | {N>=3} ;
 		T^{N} -> | {N>=2};
	        T ( S T )^{N-3}  -> S | { N >= 3};
                ( S T )^{N-3} S -> T^{N-1} |{N - 3 >= 0};
		( S T )^{ N - 3} ->  ( T )^{N-1} S |{N - 4 >= 0};
		T ( S T )^{N-4} -> ( T )^{ N - 1} S | {N - 4 >= 0};

"
 	;

let Rnorm = psrs S "";

pconfluent_ext R Rnorm;
(* it is not confluent *)

pword_completion R;

