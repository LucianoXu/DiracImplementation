(* Tests for reducibility.*)

let N = parameters "P";

let S = pword_signature N "a{i} | {0<= i } " ;

let R = psrs S 
	"a{j} -> a{k} | { j<= k <=  P}";

let u = pword S "a{i} | {i<= P}";
preducible S R u ; (* is true *)

let u = pword S "a{i}|{i>=0}";
preducible S R u ; (* is false *)

let u = pword S "a{i+1} a{i} |{i<=P }";
preducible S R u; (* is true *)
