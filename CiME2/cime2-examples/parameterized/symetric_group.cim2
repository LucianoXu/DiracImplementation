(* This needs some finite products *)

let N = parameters "N";

let S = pword_signature N "a{i} | { 1 <=i <= N-1}" ;

let R = psrs S " a{i} a{i}  -> | {N-1>=i>=1};
		 a{j} a{i}  -> a{i} a{j} | {N-1>=j>i /\ i >=1 /\ j<>i+1};
		(a{i} a{i+1})^{3} -> | {N-2>=i >=1 };
		";

pconfluent R; (* This is not confluent *)

"end";
#quit;



