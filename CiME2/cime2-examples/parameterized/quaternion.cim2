(* Group of quaternion $D_{2N}$ (see "Finite modular data" [coste-gannon...])*)

let N = parameters "N";

let S = pword_signature N "r | {N>=2}; s | {N>=2}" ;

let R = psrs S "r^{2.N} -> | {N>=2};
		s s -> r^{N}| {N>=2};
 		r s  -> s r^{2.N-1} | {N>=2};"
 	;

let Rnorm = psrs S "";

pconfluent_ext R Rnorm;
(* it is not confluent *)
