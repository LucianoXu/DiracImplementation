operators
  + : AC
  0 : constant
  - : unary
  x,y : variable

  a,b : constant

 theory AG(+,0,-)

axioms
%  x+0    = x;
%  x+-(x) = 0;

  a+a=b+b+b+x;

order 
  rpo( b > a > - > 0 > + ; + mul)

conjectures
  a=0;
  b=0;

end

