% Abelian groups theory modulo AC
%
% $Id: groupes-abeliens-AC.cim,v 1.1 1997/11/26 13:05:38 marche Exp $
% 
%***************************************************************************

operators
  + : AC
  0 : constant
  - : unary
  x,y,z : variable

axioms
  x+0    = x;
  x+-(x) = 0;

order 
  rpo( - > + > 0 ; + mul)

conjectures 

  -(0) = 0 ;
  -(-(x)) = x ;
  -(x+y)+-(x+z)+x = -(x+y+z);
  x+x = 0 ;

end

