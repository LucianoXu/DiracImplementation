operators

  0 : constant
  s : unary
  +,* : AC

  x,y : variable

problems

 dp_termination(
   x + 0 -> x ;
   x + s(y) -> s(x+y) ;
   x * 0 -> 0 ;
   x * s(y) -> (x*y)+x );

order

  poly( [0] = 0; [s](x) = x+1; [+](x,y)=x+y; [*](x,y)=x.y;
        [TOP(+)](x,y)=x+y; [TOP(*)](x,y)=x.y+x+y )


problems

  compare  x + 0 with  x ;
  compare  x + s(y) with s(x+y) ;
  compare  x * 0 with 0 ;
  compare  x * s(y) with (x*y)+x ;

end
